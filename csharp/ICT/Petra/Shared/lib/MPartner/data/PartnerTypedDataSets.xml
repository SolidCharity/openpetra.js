      <Field sqltable="p_partner" sqlfield="s_date_created_d"/>
      <!-- used for displaying 'Partner Updated' field -->
      <Field sqltable="p_partner" sqlfield="s_date_modified_d"/>
      <!-- used for displaying 'Partner Updated' field -->
      <Field sqltable="p_person" sqlfield="p_date_of_birth_d"/>
